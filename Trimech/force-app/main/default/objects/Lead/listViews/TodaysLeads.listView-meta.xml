<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TodaysLeads</fullName>
    <booleanFilter>1 AND 2 AND ((3 AND 4) OR 5) AND 6</booleanFilter>
    <columns>FULL_NAME</columns>
    <columns>LEAD.CREATED_DATE</columns>
    <columns>LEAD.LAST_UPDATE</columns>
    <columns>Campaign_Type__c</columns>
    <columns>Lead_Product_Category__c</columns>
    <columns>Lead_Product_of_Interest__c</columns>
    <columns>UPDATEDBY_USER.ALIAS</columns>
    <columns>Pardot_Lightning_URL__c</columns>
    <columns>Import_Date_Time__c</columns>
    <columns>Task_Import_Date_Time__c</columns>
    <columns>LEAD.LEAD_SOURCE</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>LEAD.CREATED_DATE</field>
        <operation>equals</operation>
        <value>LAST_N_DAYS:7</value>
    </filters>
    <filters>
        <field>Campaign_Type__c</field>
        <operation>contains</operation>
        <value>MQL,SQL,UQL</value>
    </filters>
    <filters>
        <field>Lead_Product_Category__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>Lead_Product_of_Interest__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>Task_Owner__c</field>
        <operation>notEqual</operation>
    </filters>
    <filters>
        <field>LEAD.STATUS</field>
        <operation>notEqual</operation>
        <value>Unqualified,Disqualified</value>
    </filters>
    <label>Today&apos;s Leads</label>
    <sharedTo>
        <group>Marketing</group>
        <group>Sales_Ops</group>
    </sharedTo>
</ListView>
